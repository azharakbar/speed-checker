{
    "manifest_version": 2,
    "name": "Speed Test",
    "description": "An extension to reflect the current speed of your internet",
    "version": "1.1",
    "icons": {
        "32": "icons/icon_32.png",
        "48": "icons/icon_48.png",
        "64": "icons/icon_64.png",
        "96": "icons/icon_96.png"
    },
    "background":{
        "scripts":["background.js"]
    },
    "browser_action":{
        "default_icon": {
            "64": "icon_64.png"
        },
        "default_title": "My Speed"
    }
}
